type Query {
  quoteByName(name: String!): Quote
}

type Mutation {
  saveQuote(input: QuoteInput!): Quote
  deleteQuote(name: String!): String
}

input QuoteInput {
  name: String!
  content: String!
  author: String
  contributor: String!
}

type Quote {
  name: String
  content: String
  author: String
  contributor: String
}
